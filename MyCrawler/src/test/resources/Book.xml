<?xml version="1.0" encoding="UTF-8"?>
<excel name="Book"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://www.renlm.cn/schema/MyExcel.xsd">
	<sheet name="起点小说网">
		<column field="name">
			<title>书名</title>
		</column>
		<column field="author">
			<title>作者</title>
		</column>
		<column field="wordNumber">
			<title>字数</title>
		</column>
		<column field="wordNumberUnit">
			<title>字数单位</title>
		</column>
		<column field="intro">
			<title>简介</title>
		</column>
		<column field="cover">
			<title>封面链接</title>
		</column>
		<column field="url">
			<title>访问链接</title>
		</column>
	</sheet>
</excel>